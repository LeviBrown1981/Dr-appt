<h1><%= @doctor.name %>: Appts</h1>
<%= link_to 'Add Appointment', new_doctor_appt_path(@doctor) %>
<hr />

<h2>new_doctor_appt_path</h2>
<ul>
  <% @doctors.each do |doctor| %>
    <li>
      <%= doctor.user.full_name %>
      <br />
      <%= link_to 'Remove', doctor_appt_path(@doctor, doctor), method: :delete %>
    </li>
  <% end %>
</ul>
